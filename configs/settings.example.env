# Azure SQL (ODBC via pyodbc)
# DO NOT COMMIT REAL SECRETS. Copy to .env locally and fill in values.

DB_CONN="Driver={ODBC Driver 18 for SQL Server};Server=tcp=<server>.database.windows.net,1433;Database=<database>;Uid=<sql_user>;Pwd=<password>;Encrypt=yes;TrustServerCertificate=no;Connection Timeout=30;"

# Logging
LOG_LEVEL=INFO

# Optional: default output directory for generated JSONL
OUT_DIR=data/out
